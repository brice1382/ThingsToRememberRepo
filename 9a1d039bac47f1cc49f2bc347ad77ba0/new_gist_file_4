 // GET: ServiceOrders
        public ActionResult SearchPanel(string searchAction, string FeatureCode, string SearchID = "")
        {
            var myServiceAPI = new MFS.BusinessAPI.MyServiceAPI();
            var userFilter = myServiceAPI.GetDefaultSearch(SearchID);
            var config = SearchFilterConfigurationProvider.GetConfigurationByFeatureCode(FeatureCode);

            var vm = new SearchPanelVM(config, userFilter)
            {
                SearchAction = searchAction,
                FeatureCode = FeatureCode
            };
            vm.SetFilterValue(SearchPanelFilter.DateRange, -30);
            return PartialView("SearchPanel", vm);
        }